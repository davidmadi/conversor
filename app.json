{
  "expo": {
    "githubUrl": "https://github.com/davidmadi/conversor",
    "name": "Cook Conversor",
    "ios": {
      "bundleIdentifier": "com.davidmadi.cookconversor",
      "supportsTablet": true
    },
    "version": "1.0.3",
    "slug": "conversor",
    "loading": {
      "hideExponentText": true,
      "backgroundImage": "./assets/background-pattern.png"
    },
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/logo167.png",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/splash.png",
      "backgroundColor": "#dafff0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}